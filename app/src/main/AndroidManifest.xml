<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="paste3.bakersbox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ViewRecipeByCatagoryActivity"></activity>
        <activity android:name=".ViewIngredientActivity" />
        <activity android:name=".QuoteActivity" />
        <activity android:name=".EditRecipeActivity" />
        <activity android:name=".ViewRecipeActivity" />
        <activity android:name=".CreateRecipeActivity" />
        <activity android:name=".CreateIngredientActivity" />
        <activity android:name=".IngredientsActivity" />
        <activity
            android:name=".RecipeActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="andoird.support.PARENT_ACTIVITY"
                android:value="paste3.bakersbox.MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>